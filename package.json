    "bcryptjs": "^3.0.3",
    "bullmq": "^5.25.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.1.0",
    "ioredis": "^5.4.1",
    "jose": "^6.1.3",
    "openai": "^4.72.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
